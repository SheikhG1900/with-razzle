{
  "compilerOptions": {
    "outDir": "./built",
    "allowJs": true,
    "checkJs": false,
    "target": "esnext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports":true,
    "strictNullChecks": true,
    "sourceMap": true,
    "jsx": "preserve",
    "baseUrl": "./src",
    "paths": {
      "@/*": ["./apps/@main/*"]
    }
  },
  "exclude": [
    "build"
  ]
}

